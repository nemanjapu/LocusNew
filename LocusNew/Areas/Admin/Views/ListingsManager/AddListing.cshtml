                        <label for="Internet">Internet</label>
                    </div>
                </div>

                <div class="form-group">
                    <div class="inputGroup">
                        @Html.CheckBoxFor(m => m.Furniture)
                        <label for="Furniture">Namještaj</label>
                    </div>
                </div>
            </div>
            <div class="col-sm-6 col-xl-3">
                <div class="form-group">
                    <div class="inputGroup">
                        @Html.CheckBoxFor(m => m.Videophone)
