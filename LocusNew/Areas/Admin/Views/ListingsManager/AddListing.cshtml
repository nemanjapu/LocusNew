            <div class="col-sm-6 col-xl-3">
                <div class="form-group">
                    <div class="inputGroup">
                        @Html.CheckBoxFor(m => m.Pantry)
                        <label for="Pantry">Ostava</label>
                    </div>
                </div>

                <div class="form-group">
