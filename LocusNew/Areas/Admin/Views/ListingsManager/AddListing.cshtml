                <div class="form-group">
                    <div class="inputGroup">
                        @Html.CheckBoxFor(m => m.CableTV)
                        <label for="CableTV">Kablovska TV</label>
                    </div>
                </div>

                <div class="form-group">
                    <div class="inputGroup">
